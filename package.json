{
  "name": "@develit-io/nitro-guards",
  "version": "1.0.2",
  "description": "Nitro Guards",
  "repository": "@develit-io/nitro-guards",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
